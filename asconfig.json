{
  "entries": [
    "./assembly/entity.ts"
  ],
  "targets": {
    "debug": {
      "outFile": "build/entity.debug.wasm",
      "textFile": "build/entity.debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "build/entity.release.wasm",
      "textFile": "build/entity.release.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "importMemory": true,
    "sharedMemory": true,
    "initialMemory": true,
    "maximumMemory": true,
    "noExportMemory": true,
    "lib": [
      "../node_modules/std/assembly/"
    ],
    "enable": [
      "threads"
    ],
    "transform": [
      "./start_transform.mjs"
    ]
  }
}